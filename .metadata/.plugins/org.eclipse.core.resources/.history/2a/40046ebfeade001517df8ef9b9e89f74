	<select id="findHosArchiveServiceItemCost" parameterType="Map"
		resultMap="BaseResultMap">
		select * from HosArchiveServiceItemCost
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID = #{serialid}
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="coststate !=null ">
				and CostState = ${coststate}
			</if>
			<!-- empid是当前医生的ID，由于实时明细费用表ID的前几位是开处方的医生ID， 所以这样可以确保加载出的 信息，是由当前医生开处方时产生的 -->
			<if test="empid !=null and empid !=''">
				and SerialID like #{empid}
			</if>
			<if test="adviceid !=null and adviceid !=''">
				and AdviceID=#{adviceid}
			</if>
			<if test="isattach !=null ">
				and IsAttach = #{isattach}
			</if>
			<if test="medicineadviceid !=null and medicineadviceid !=''">
				and MedicineAdviceId = #{medicineadviceid}
			</if>
			<!-- 0 其他 1 检查 2 检验 3 手术 -->
			<if test="serviceitemtype !=null">
				and ServiceItemType = ${serviceitemtype}
			</if>
			<!-- 用于检索需要打印发票的记录 -->
			<if test="invoicestate != null">
				and InvoiceState = ${invoicestate}
			</if>
			<if test="adviceserialid !=null and adviceserialid !=''">
				and AdviceSerialID=#{adviceserialid}
			</if>
			<!-- 用于检索正常收费的项目 -->
			<if test="costmode != null">
				and CostState > 0
			</if>

		</where>
		<if test="start!=null and size!=null">
			order by SerialID DESC offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalHosArchiveServiceItemCost" parameterType="Map"
		resultType="Long">
		select count(*) from HosArchiveServiceItemCost
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID = #{serialid}
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="coststate !=null ">
				and CostState = ${coststate}
			</if>
			<!-- empid是当前医生的ID，由于实时明细费用表ID的前几位是开处方的医生ID， 所以这样可以确保加载出的 信息，是由当前医生开处方时产生的 -->
			<if test="empid !=null and empid !=''">
				and SerialID like #{empid}
			</if>
			<if test="adviceid !=null and adviceid !=''">
				and AdviceID=#{adviceid}
			</if>
			<if test="isattach !=null ">
				and IsAttach = #{isattach}
			</if>
			<if test="medicineadviceid !=null and medicineadviceid !=''">
				and MedicineAdviceId = #{medicineadviceid}
			</if>
			<!-- 0 其他 1 检查 2 检验 3 手术 -->
			<if test="serviceitemtype !=null">
				and ServiceItemType = ${serviceitemtype}
			</if>
			<if test="adviceserialid !=null and adviceserialid !=''">
				and AdviceSerialID=#{adviceserialid}
			</if>
		</where>
	</select>

	<select id="getMaxID" parameterType="Map" resultType="String">
		select MAX(SerialID) from HosArchiveServiceItemCost
		<where>
			<if test="prefix !=null and prefix !=''">
				and SerialID like #{prefix}
			</if>
		</where>
	</select>

	<select id="getExistAttached" parameterType="Map" resultType="Long">
		select count(*) from HosArchiveServiceItemCost
		<where>
			<if test="isgroupprice !=null ">
				and IsGroupPrice = #{isgroupprice}
			</if>
			<if test="isattach !=null ">
				and IsAttach = #{isattach}
			</if>
			<if test="medicineadviceid !=null and medicineadviceid !=''">
				and MedicineAdviceId = #{medicineadviceid}
			</if>
		</where>
	</select>
</mapper>