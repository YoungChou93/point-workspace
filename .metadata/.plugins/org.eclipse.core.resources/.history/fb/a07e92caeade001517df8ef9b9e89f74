	<select id="findOutpatientInvoice" parameterType="Map"
		resultMap="BaseResultMap">
		select * from OutpatientInvoice
		<where>
			<!-- 按门诊发票分类名称查询 -->
			<if test="outpatientname!=null and outpatientname!=''">
				and OutpatientName like #{outpatientname}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by OutpatientID offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalOutpatientInvoice" parameterType="Map"
