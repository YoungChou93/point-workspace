 	<select id="findDrugRoomLost" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRoomLost
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<!-- 根据入库操作员查询入库记录 -->
			<if test="executeempid !=null and executeempid !=''">
				and ExecuteEmpID = #{executeempid}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>
			<if test="loststate !=null ">
				and LostState = #{loststate}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by LostID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

