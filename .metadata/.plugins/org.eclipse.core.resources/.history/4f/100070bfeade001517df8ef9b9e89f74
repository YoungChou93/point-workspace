	<select id="findHosGuarantor" parameterType="Map" resultMap="BaseResultMap">
		select * from HosGuarantor
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID = #{serialid}
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="patientid !=null and patentid !=''">
				and PatientID = #{patientid}
			</if>
			<if test="grtname !=null and grtname !=''">
				and GrtName = #{grtname}
			</if>
			<if test="grtcertid !=null and grtcertid !=''">
				and GrtCertID = #{grtcertid}
			</if>
			<!-- 按病人所在科室检索担保人信息 -->
			<if test="regdeptid !=null and regdeptid !=''">
				and HosCode in (select HosCode from HosRegister where
				RegDeptID= #{regdeptid})
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by SerialID offset ${start} rows fetch next
			${size}
			rows only
		</if>
	</select>

	<select id="getTotalHosGuarantor" parameterType="Map"
		resultType="Long">
		select count(*) from HosGuarantor
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID = #{serialid}
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="patientid !=null and patentid !=''">
				and PatientID = #{patientid}
			</if>
			<if test="grtname !=null and grtname !=''">
				and GrtName = #{grtname}
			</if>
			<if test="grtcertid !=null and grtcertid !=''">
				and GrtCertID = #{grtcertid}
			</if>
		</where>
	</select>

	<select id="getSerialID" parameterType="Map" resultType="String">
		select MAX(SerialID) from HosGuarantor
		<where>
			<if test="prefix!=null and prefix!='' ">
				and SerialID like #{prefix}
			</if>
		</where>
	</select>
