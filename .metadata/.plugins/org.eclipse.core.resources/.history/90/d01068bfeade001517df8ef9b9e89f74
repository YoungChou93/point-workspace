	<select id="findDrugRoomTrans" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRoomTrans
		<where>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>
			<if test="origindept !=null and origindept !=''">
				and OriginDept = #{origindept}
			</if>
			<if test="destdept !=null and destdept !=''">
				and DestDept = #{destdept}
			</if>
			<if test="applystate !=null ">
				and ApplyState = ${applystate}
			</if>
			<if test="applyemp !=null and applyemp !=''">
				and ApplyEmp = #{applyemp}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by ApplyID offset ${start} rows fetch next ${size}
			rows only
		</if>
	</select>

	<select id="getTotalDrugRoomTrans" parameterType="Map"
		resultType="Long">
