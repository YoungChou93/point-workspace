	  <select id="findMedType" parameterType="Map" resultMap="BaseResultMap">
		select * from MedType
		<where>
			<!-- 按ID查询 -->
			<if test="typeid!=null and typeid!=''">
				and TypeID like #{typeid}
			</if>
			<if test="name!=null and name!='' ">
				and TypeName like #{name}
			</if>
			<if test="pid!=null and pid!='' ">
				and ParentTypeID like #{pid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by TypeID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>
	
	<select id="getTotalMedType" parameterType="Map" resultType="Long">
		select count(*) from MedType
		<where>
			<!-- 按ID查询 -->
			<if test="typeid!=null and typeid!=''">
				and TypeID like #{typeid}
			</if>
			<if test="name!=null and name!='' ">
				and TypeName like #{name}
			</if>
			<if test="pid!=null and pid!='' ">
				and ParentTypeID like #{pid}
			</if>
		</where>
	</select>
	
	<select id="getUnused1Existing" parameterType="Map" resultType="Long">
		select count(*) from MedType
		<where>
			<if test="pid!=null and pid!=''">
				and ParentTypeID = #{pid}
			</if>
			<if test="unused1!=null and unused1!='' ">
				and UnUsed1 = #{unused1}
			</if>
		</where>
	</select>
	
	
	<select id="getMaxMedTypeID" parameterType="Map"
		resultType="String">
		select MAX(TypeID) from MedType
			<where>
			<!-- 依照前缀搜索 -->
			<if test="pid!=null and pid!=''">
				and TypeID like #{pid}
			</if>
		</where>
	</select>
