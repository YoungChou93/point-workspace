	<select id="findSurgeryInformation" parameterType="Map" resultMap="BaseResultMap">
		select * from SurgeryInformation
		<where>
			<!-- 按检验分类名称查询 -->
			<if test="surgeryinfname!=null and surgeryinfname!=''">
				and SurgeryInfName like #{surgeryinfname}
			</if>
			<!-- 按检验分类ID查询 -->
			<if test="surgeryinfid!=null and surgeryinfid!=''">
				and SurgeryInfID like #{surgeryinfid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by SurgeryInfID offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalSurgeryInformation" parameterType="Map"
		resultType="Long">
		select count(*) from SurgeryInformation
		<where>
			<if test="surgeryinfid!=null and surgeryinfid!='' ">
				and SurgeryInfID like #{surgeryinfid}
			</if>
		</where>
	</select>

	<select id="selectMaxsurgeryinfid" resultType="String">
