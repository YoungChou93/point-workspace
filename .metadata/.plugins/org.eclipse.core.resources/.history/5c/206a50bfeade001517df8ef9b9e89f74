  <select id="findBatchItem" parameterType="Map"
		resultMap="BaseResultMap">
		select * from BatchItem
		<where>
			<if test="batchkind !=null ">
				and BatchKind =#{batchkind}
			</if>
			<if test="batchtype !=null ">
				and BatchType =#{batchtype}
			</if>
			<if test="batchclass !=null ">
				and BatchClass =#{batchclass}
			</if>
			<if test="empid !=null and empid !=''">
				and EmpID=#{empid}
			</if>
			<if test="deptid !=null and deptid !=''">
				and DeptID=#{deptid}
			</if>
			<if test="filter !=null and filter !=''">
				and (PyCode like #{filter} or WbCode like #{filter} or BatchName like #{filter} )
			</if>
			<if test="bestate !=null ">
				and BeState = ${bestate}
			</if>
				and BeState != -1
		</where>
		<if test="start!=null and size!=null">
			order by SerialID DESC offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalBatchItem" parameterType="Map"
		resultType="Long">
		select count(*) from BatchItem
		<where>
			<if test="batchkind !=null ">
				and BatchKind =#{batchkind}
			</if>
			<if test="batchtype !=null ">
				and BatchType =#{batchtype}
			</if>
			<if test="batchclass !=null ">
				and BatchClass =#{batchclass}
			</if>
			<if test="empid !=null and empid !=''">
				and EmpID=#{empid}
			</if>
			<if test="deptid !=null and deptid !=''">
				and DeptID=#{deptid}
			</if>
			<if test="filter !=null and filter !=''">
				and (PyCode like #{filter} or WbCode like #{filter} or BatchName like #{filter} )
			</if>
			<if test="bestate !=null ">
				and BeState = ${bestate}
			</if>
			and BeState != -1
		</where>
	</select>

	<select id="getMaxID" parameterType="Map" resultType="String">
		select MAX(SerialID) from BatchItem
		<where>
			<if test="prefix !=null and prefix !=''">
				and SerialID like #{prefix}
			</if>
		</where>
	</select>
  
  
