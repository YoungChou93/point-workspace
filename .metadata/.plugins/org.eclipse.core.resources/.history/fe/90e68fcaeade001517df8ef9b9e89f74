	<select id="findMedicalClass" parameterType="Map" resultMap="BaseResultMap">
		select * from MedicalClass
		<where>
			<!-- 按病案分类名称查询 -->
			<if test="medicalname!=null and medicalname!=''">
				and MedicalName like #{medicalname}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by MedicalID offset ${start} rows fetch next ${size}
			rows only
		</if>
	</select>

	<select id="getTotalMedicalClass" parameterType="Map"
		resultType="Long">
		select count(*) from MedicalClass
		<where>
			<!-- 按病案分类名称查询 -->
			<if test="medicalname!=null and medicalname!=''">
				and MedicalName like #{medicalname}
			</if>
		</where>
	</select>
	<select id="selectMaxmedicalid" resultType="String">
