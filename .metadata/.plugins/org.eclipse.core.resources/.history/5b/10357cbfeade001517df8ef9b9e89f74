  <select id="findDrugRoomOutput" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRoomOutput
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="pycode !=null and pycode !=''">
				and MedicineID in ( select MedicineID from MedicineBaseInfo where PyCode like #{pycode} )
			</if>
			<if test="outputid !=null and outputid !=''">
				and OutputID like #{outputid}
			</if>
			<if test="deptid !=null and deptid !=''">
				and DeptID like #{deptid}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode like #{billcode}
			</if>
			<if test="outputtime !=null and outputtime !=''">
				and OutputID like #{outputtime}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by OutputID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

