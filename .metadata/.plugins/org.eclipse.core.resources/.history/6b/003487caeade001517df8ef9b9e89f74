	<select id="findCheckClass" parameterType="Map" resultMap="BaseResultMap">
		select * from CheckClass
		<where>
			<!-- 按核算分类名称查询 -->
			<if test="checkname!=null and checkname!=''">
				and CheckName like #{checkname}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by CheckID offset ${start} rows fetch next ${size}
			rows only
		</if>
	</select>

	<select id="getTotalCheckClass" parameterType="Map" resultType="Long">
		select count(*) from CheckClass
		<where>
			<!-- 按核算分类名称查询 -->
			<if test="checkname!=null and checkname!=''">
				and CheckName like #{checkname}
			</if>
		</where>
	</select>
	<select id="selectMaxcheckid" resultType="String">
		select MAX(CheckID)
		from CheckClass
	</select>
