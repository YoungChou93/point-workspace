	<select id="findDrugRepoBack" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRepoBack
		<where>
			<!-- 根据确认认操作员查询入库记录 -->
			<if test="backempid !=null and backempid !=''">
				and BackEmpID = #{backempid}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>
			<if test="backstate !=null ">
				and BackState = #{backstate}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by BackID DESC offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

