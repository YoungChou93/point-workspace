	<select id="findDocSchedule" parameterType="Map" resultMap="BaseResultMap">
		select * from BDocSchedule
		<where>
			<if test="syncempid !=null and syncempid !=''">
				and SyncEmpID = #{syncempid}
			</if>
			<if test="monamdept !=null and monamdept !=''">
				or MonAmDept = #{monamdept}
			</if>
			<if test="monpmdept !=null and monpmdept !=''">
				or MonPmDept = #{monpmdept}
			</if>
			<if test="tueamdept !=null and tueamdept !=''">
				or TueAmDept = #{tueamdept}
			</if>
			<if test="tuepmdept !=null and tuepmdept !=''">
				or TuePmDept = #{tuepmdept}
			</if>
			<if test="wedamdept !=null and wedamdept !=''">
				or WedAmDept = #{wedamdept}
			</if>
			<if test="wedpmdept !=null and wedpmdept !=''">
				or WedPmDept = #{wedpmdept}
			</if>
			<if test="thuamdept !=null and thuamdept !=''">
				or ThuAmDept = #{thuamdept}
			</if>
			<if test="thupmdept !=null and thupmdept !=''">
				or ThuPmDept = #{thupmdept}
			</if>
			<if test="friamdept !=null and friamdept !=''">
				or FriAmDept = #{friamdept}
			</if>
			<if test="fripmdept !=null and fripmdept !=''">
				or FriPmDept = #{fripmdept}
			</if>
			<if test="satamdept !=null and satamdept !=''">
				or SatAmDept = #{satamdept}
			</if>
			<if test="satpmdept !=null and satpmdept !=''">
				or SatPmDept = #{satpmdept}
			</if>
			<if test="sunamdept !=null and sunamdept !=''">
				or SunAmDept = #{sunamdept}
			</if>
			<if test="sunpmdept !=null and sunpmdept !=''">
				or SunPmDept = #{sunpmdept}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by SyncEmpID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

	<select id="getTotalDocSchedule" parameterType="Map" resultType="Long">
		select count(*) from BDocSchedule
		<where>
			<if test="syncempid !=null and syncempid !=''">
				and SyncEmpID = #{syncempid}
			</if>
			<if test="monamdept !=null and monamdept !=''">
				or MonAmDept = #{monamdept}
			</if>
			<if test="monpmdept !=null and monpmdept !=''">
				or MonPmDept = #{monpmdept}
			</if>
			<if test="tueamdept !=null and tueamdept !=''">
				or TueAmDept = #{tueamdept}
			</if>
			<if test="tuepmdept !=null and tuepmdept !=''">
				or TuePmDept = #{tuepmdept}
			</if>
			<if test="wedamdept !=null and wedamdept !=''">
				or WedAmDept = #{wedamdept}
			</if>
			<if test="wedpmdept !=null and wedpmdept !=''">
				or WedPmDept = #{wedpmdept}
			</if>
			<if test="thuamdept !=null and thuamdept !=''">
				or ThuAmDept = #{thuamdept}
			</if>
			<if test="thupmdept !=null and thupmdept !=''">
				or ThuPmDept = #{thupmdept}
			</if>
			<if test="friamdept !=null and friamdept !=''">
				or FriAmDept = #{friamdept}
			</if>
			<if test="fripmdept !=null and fripmdept !=''">
				or FriPmDept = #{fripmdept}
			</if>
			<if test="satamdept !=null and satamdept !=''">
				or SatAmDept = #{satamdept}
			</if>
			<if test="satpmdept !=null and satpmdept !=''">
				or SatPmDept = #{satpmdept}
			</if>
			<if test="sunamdept !=null and sunamdept !=''">
				or SunAmDept = #{sunamdept}
			</if>
			<if test="sunpmdept !=null and sunpmdept !=''">
				or SunPmDept = #{sunpmdept}
			</if>
		</where>
	</select>
	
	<delete id="deleteByMap" parameterType="Map" >
		delete  from BDocSchedule
		<where>
			<if test="syncempid !=null and syncempid !=''">
				and SyncEmpID = #{syncempid}
			</if>
		</where>
	</delete>
</mapper>