	<select id="findAPatientInfo" parameterType="Map" resultMap="BaseResultMap">
		select * from APatientInfo
		<where>
			<if test="empid!=null and empid!=''">
				and EmpID = #{empid}
			</if>
			<if test="name!=null and name!=''">
				and Name like #{name}
			</if>
			<if test="idtype!=null and idtype!=''">
				and IDType = #{idtype}
			</if>
			<if test="idcode!=null and idcode!=''">
				and IDCode = #{idcode}
			</if>
			<if test="iccardid !=null and iccardid !=''">
				and ICCardID like #{iccardid}
			</if>
			<if test="check !=null">
				and ICCardID is not null
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by PatientID offset ${start} rows fetch next ${size}
			rows only
		</if>
	</select>

	<select id="getTotalAPatientInfo" parameterType="Map"
		resultType="Long">
		select count(*) from APatientInfo
		<where>
			<if test="empid!=null and empid!=''"> and
				EmpID = #{empid}
			</if>
			<if test="idtype!=null and idtype!=''">
				and IDType = #{idtype}
			</if>
			<if test="idcode!=null and idcode!=''">
				and IDCode = #{idcode}
			</if>
		</where>
	</select>

	<select id="generateCode" resultType="String">
		select MAX(PatientID)
		from
		APatientInfo
	</select>

	<select id="getAPatientInfo" parameterType="Map" resultMap="BaseResultMap">
		select * from APatientInfo
		<where>
			<if test="idtype!=null and idtype!=''">
				and IDType = #{idtype}
			</if>
			<if test="idcode!=null and idcode!=''">
				and IDCode = #{idcode}
			</if>
		</where>
	</select>
</mapper>