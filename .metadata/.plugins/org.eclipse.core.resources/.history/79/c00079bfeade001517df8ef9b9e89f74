	<select id="findAirServiceItemCost" parameterType="Map"
		resultMap="BaseResultMap">
		select * from AirServiceItemCost
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID=#{serialid}
			</if>
			<!-- 为了避免检索出病人以前就诊遗留的费用信息，规定检索出最大的就诊号 -->
			<if test="iccardid !=null and iccardid !=''">
				and MarkID in (select MAX(MarkID) from AirMedicineCost where ICCardID = #{iccardid})
			</if>
			<if test="patientid !=null and patientid !=''">
				and PatientID=#{patientid}
			</if>
			<if test="markid !=null and markid !=''">
				and MarkID=#{markid}
			</if>
			<if test="coststate !=null">
				and CostState = ${coststate}
			</if>
			<if test="prescriptionid !=null and prescriptionid !=''">
				and PrescriptionID=#{prescriptionid}
			</if>
			<if test="adviceid !=null and adviceid !=''">
				and AdviceID=#{adviceid}
			</if>
			<if test="costid !=null and costid !=''">
				and ServiceItemCostID=#{costid}
			</if>
			<!-- 0 其他 1 检查 2 检验 3 手术  -->
			<if test="serviceitemtype !=null">
				and ServiceItemType = ${serviceitemtype}
			</if>
			<if test="serviceitemid !=null and serviceitemid !=''">
				and ServiceItemID=#{serviceitemid}
			</if>
			<!-- 用于医技科室检索该科室相关费用明细 -->
			<if test="exedeptid !=null and exedeptid !=''">
				and (CheckApplyID in(select ApplyID from CheckApply where ExecuteDeptID=#{exedeptid}) 
				or TestApplyID in(select ApplyID from TestApply where ExecuteDeptID=#{exedeptid}))
			</if>
			<!-- empid是当前医生的ID，由于实时明细费用表ID的前几位是开处方的医生ID， 所以这样可以确保加载出的 信息，是由当前医生开处方时产生的 -->
			<if test="empid !=null and empid !=''">
				and SerialID like #{empid}
			</if>
			<!-- 用于检索需要打印发票的记录 -->
			<if test="invoicestate != null">
				and InvoiceState = ${invoicestate}
			</if>
			<!-- 用于支持以处方为单位检索费用记录 -->
			<if test="ids != null">
				and PrescriptionID in
				<foreach item="item" index="index" collection="ids" open="("
					separator="," close=")">
					#{item}
				</foreach>
			</if>
			<if test="medicineadviceid != null and medicineadviceid !=''">
				and MedicineAdviceID = #{medicineadviceid}
			</if>
			<if test="isattach != null">
				and IsAttach = #{isattach}
			</if>
			<if test="adviceserialid != null and adviceserialid !=''">
				and AdviceSerialID = #{adviceserialid}
			</if>
			<if test="isgroupprice !=null">
				and IsGroupPrice = #{isgroupprice}
			</if>
			
		</where>
		order by OutpatientID DESC
		<if test="start!=null and size!=null">
			offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

	<select id="getTotalAirServiceItemCost" parameterType="Map"
		resultType="Long">
		select count(*) from AirServiceItemCost
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID=#{serialid}
			</if>
			<if test="iccardid !=null and iccardid !=''">
				and ICCardID=#{iccardid}
			</if>
			<if test="patientid !=null and patientid !=''">
				and PatientID=#{patientid}
			</if>
			<if test="markid !=null and markid !=''">
				and MarkID=#{markid}
			</if>
			<if test="prescriptionid !=null and prescriptionid !=''">
				and PrescriptionID=#{prescriptionid}
			</if>
			<if test="adviceid !=null and adviceid !=''">
				and AdviceID=#{adviceid}
			</if>
			<if test="costid !=null and costid !=''">
				and ServiceItemCostID=#{costid}
			</if>
			<!-- 0 其他 1 检查 2 检验 3 手术  -->
			<if test="serviceitemtype !=null">
				and ServiceItemType = ${serviceitemtype}
			</if>
			<!-- 用于医技科室检索该科室相关费用明细 -->
			<if test="exedeptid !=null and exedeptid !=''">
				and (CheckApplyID in(select ApplyID from CheckApply where ExecuteDeptID=#{exedeptid}) 
				or TestApplyID in(select ApplyID from TestApply where	ExecuteDeptID=#{exedeptid}))
			</if>
			<if test="serviceitemid !=null and serviceitemid !=''">
				and ServiceItemID=#{serviceitemid}
			</if>
			<if test="coststate !=null">
				and CostState = ${coststate}
			</if>
			<!-- empid是当前医生的ID，由于实时明细费用表ID的前几位是开处方的医生ID， 所以这样可以确保加载出的 信息，是由当前医生开处方时产生的 -->
			<if test="empid !=null and empid !=''">
				and SerialID like #{empid}
			</if>
			<if test="medicineadviceid != null and medicineadviceid !=''">
				and MedicineAdviceID = #{medicineadviceid}
			</if>
			<if test="isattach != null">
				and IsAttach = #{isattach}
			</if>
			<if test="isgroupprice !=null">
				and IsGroupPrice = #{isgroupprice}
			</if>
			
		</where>
	</select>

	<select id="getExistAttached" parameterType="Map" resultType="Long">
		select count(*) from AirServiceItemCost
		<where>
			<if test="isgroupprice !=null ">
				and IsGroupPrice = #{isgroupprice}
			</if>
			<if test="isattach !=null ">
				and IsAttach = #{isattach}
			</if>
			<if test="medicineadviceid !=null and medicineadviceid !=''">
				and MedicineAdviceId = #{medicineadviceid}
			</if>
		</where>
	</select>

	<select id="getSerialID" parameterType="Map" resultType="String">
		select MAX(SerialID) from AirServiceItemCost
		<where>
			<if test="prefix !=null and prefix !=''">
				and SerialID like #{prefix}
			</if>
		</where>
	</select>
</mapper>