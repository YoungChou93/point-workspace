			<!-- 根据采购计划ID查询Map表信息 -->
			<if test="planid !=null and planid !=''">
				and PlanID like #{planid}
			</if>
		</where>
	</select>

	<select id="getMaxBuyPlanDrugMapID" parameterType="java.lang.String"
		resultType="String">
		select Max(SerialID) from BuyPlanDrugMap where SerialID
