	<select id="findBuyPlanDrugMap" parameterType="Map" resultMap="BaseResultMap">
		select * from BuyPlanDrugMap
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID like #{serialid}
			</if>
			<!-- 根据采购计划ID查询Map表信息 -->
			<if test="planid !=null and planid !=''">
				and PlanID like #{planid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by PlanID DESC offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalBuyPlanDrugMap" parameterType="Map"
		resultType="Long">
