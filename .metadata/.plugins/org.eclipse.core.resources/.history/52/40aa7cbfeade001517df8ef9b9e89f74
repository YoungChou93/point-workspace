	<select id="findMedicineAdvice" parameterType="Map" resultMap="BaseResultMap">
		select * from MedicineAdvice
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID = #{serialid}
			</if>
			<if test="prescriptionid !=null and prescriptionid !=''">
				and PrescriptionID = #{prescriptionid}
			</if>
			<if test="adviceid !=null and adviceid !=''">
				and AdviceID = #{adviceid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by AdviceID , SerialID 
			offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

	<select id="getTotalMedicineAdvice" parameterType="Map"
		resultType="Long">
