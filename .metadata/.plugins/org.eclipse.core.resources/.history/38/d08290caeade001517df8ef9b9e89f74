						MedicineName like #{filter})
			</if>
			<if test="medicinetype !=null  ">
				and MedicineTypeID = ${medicinetype}
			</if>
		</where>		
	</select>
	
	<select id="selectPartByPrimaryKey"  parameterType="java.lang.String" resultMap="PartResultMap">
		select
		<include refid="Part_Column_List" />
		from MedicineBaseInfo
		where MedicineID = #{medicineid,jdbcType=CHAR}
	</select>
