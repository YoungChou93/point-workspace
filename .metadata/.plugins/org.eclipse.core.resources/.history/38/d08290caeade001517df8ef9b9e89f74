	<select id="findMedicineBaseInfo" parameterType="Map" resultMap="BaseResultMap">
		select * from MedicineBaseInfo
		<where>
			<!-- 按权限ID查询 -->
			<if test="id!=null and id!=''">
				and MedicineID like #{id}
			</if>
			<if test="name!=null and name!='' ">
				and MedicineName like #{name}
			</if>
			<if test="filter !=null and filter !='' ">
				and (PyCode like #{filter} OR WbCode like #{filter} OR SelfDefCode like #{filter} OR 
						MedicineName like #{filter})
			</if>
			<if test="medicinetype !=null  ">
				and MedicineTypeID = ${medicinetype}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by MedicineID offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalMedicineBaseInfo" parameterType="Map"
		resultType="Long">
		select count(*) from MedicineBaseInfo
		<where>
			<!-- 按权限ID查询 -->
			<if test="id!=null and id!=''">
				and MedicineID like #{id}
			</if>
			<if test="name!=null and name!='' ">
				and MedicineName like #{name}
			</if>
			<if test="filter !=null and filter !='' ">
				and (PyCode like #{filter} OR WbCode like #{filter} OR SelfDefCode like #{filter} OR 
						MedicineName like #{filter})
			</if>
			<if test="medicinetype !=null  ">
				and MedicineTypeID = ${medicinetype}
			</if>
		</where>
	</select>

	<select id="getMaxMedicineID" parameterType="Map" resultType="String">
		select MAX(MedicineID) from MedicineBaseInfo
		<where>
			<!-- 药剂类别id 前6bit -->
			<if test="prefix!=null and prefix!='' ">
				and MedicineID like #{prefix}
			</if>
		</where>
	</select>
	
	<select id="findMedicineBasePart" parameterType="Map" resultMap="PartResultMap">
		select <include refid="Part_Column_List" /> 
 		from MedicineBaseInfo
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="filter !=null and filter !='' ">
				and (PyCode like #{filter} OR WbCode like #{filter} OR SelfDefCode like #{filter} OR 
						MedicineName like #{filter})
			</if>
			<if test="medicinetype !=null  ">
				and MedicineTypeID = ${medicinetype}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by MedicineID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>
	
	<select id="getTotalMedicineBasePart" parameterType="Map" resultType="Long">
		select count(*) from MedicineBaseInfo
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="filter !=null and filter !='' ">
