	<select id="findCheckInformation" parameterType="Map" resultMap="BaseResultMap">
		select * from CheckInformation
		<where>
			<!-- 按检验分类名称查询 -->
			<if test="checkinfname!=null and checkinfname!=''">
				and CheckInfName like #{checkinfname}
			</if>
			<!-- 按检验分类ID查询 -->
			<if test="checkinfid!=null and checkinfid!=''">
				and CheckInfID like #{checkinfid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by CheckInfID offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalCheckInformation" parameterType="Map"
		resultType="Long">
		select count(*) from CheckInformation
		<where>
			<if test="checkinfid!=null and checkinfid!='' ">
				and CheckInfID like #{checkinfid}
			</if>
		</where>
	</select>

	<select id="selectMaxcheckinfid" resultType="String">
		select
		MAX(CheckInfID)
		from CheckInformation
	</select>
