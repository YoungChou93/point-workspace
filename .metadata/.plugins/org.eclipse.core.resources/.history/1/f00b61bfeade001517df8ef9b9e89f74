	<select id="findDrugRepoStore" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRepoStore
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="filter !=null and filter !='' and medicinetype != null">
				and MedicineID in ( 
					select MedicineID from MedicineBaseInfo 
					where ( PyCode like #{filter} or WbCode like #{filter} or 
							SelfDefCode like #{filter} or MedicineName like #{filter})
							and MedicineTypeID = ${medicinetype}
				)
			</if>
			<if test="filter !=null and filter !='' ">
				and MedicineID in ( 
					select MedicineID from MedicineBaseInfo 
					where ( PyCode like #{filter} or WbCode like #{filter} or 
							SelfDefCode like #{filter} or MedicineName like #{filter})
				)
			</if>
			<if test=" medicinetype != null">
				and MedicineID in ( 
					select MedicineID from MedicineBaseInfo 
					where MedicineTypeID = ${medicinetype}
				)
			</if>
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<!-- LimitTime > today  后减前-->
			<if test="today !=null and today !=''">
				and DATEDIFF(day,LimitTime, #{today}) <![CDATA[ < ]]> 0
			</if>
			<!-- LimitTime <= untoday  后减前-->
			<if test="untoday !=null and untoday !=''">
				and DATEDIFF(day,LimitTime, #{untoday}) <![CDATA[ >= ]]> 0
				and ( StoreNumber > 0 or PassageNumber > 0 )
			</if>
			<if test="unsafe !=null ">
				and UnsafeState != 0
			</if>				
		</where>
		<if test="start!=null and size!=null">
			order by StoreID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

	<select id="getTotalDrugRepoStore" parameterType="Map"
		resultType="Long">
		select count(*) from DrugRepoStore
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<if test="filter !=null and filter !='' and medicinetype != null">
				and MedicineID in ( 
					select MedicineID from MedicineBaseInfo 
					where ( PyCode like #{filter} or WbCode like #{filter} or 
							SelfDefCode like #{filter} or MedicineName like #{filter})
							and MedicineTypeID = ${medicinetype}
				)
			</if>
			<if test="filter !=null and filter !='' ">
				and MedicineID in ( 
					select MedicineID from MedicineBaseInfo 
					where ( PyCode like #{filter} or WbCode like #{filter} or 
							SelfDefCode like #{filter} or MedicineName like #{filter})
				)
			</if>
			<if test=" medicinetype != null">
				and MedicineID in ( 
					select MedicineID from MedicineBaseInfo 
					where MedicineTypeID = ${medicinetype}
				)
			</if>
			<!-- LimitTime > today  后减前-->
			<if test="today !=null and today !=''">
				and DATEDIFF(day,LimitTime, #{today}) <![CDATA[ < ]]> 0
			</if>
			<!-- LimitTime <= untoday  后减前-->
			<if test="untoday !=null and untoday !=''">
				and DATEDIFF(day,LimitTime, #{untoday}) <![CDATA[ >= ]]> 0
				and ( StoreNumber > 0 or PassageNumber > 0 )
			</if>
			<if test="unsafe !=null ">
				and UnsafeState != 0
			</if>			
		</where>
	</select>
	
	<select id="getTotalStore" parameterType="Map" resultType="BigDecimal">
		select Sum(StoreNumber) from DrugRepoStore
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="storeid !=null and storeid !=''">
				and StoreID = #{storeid}
			</if>			
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<!-- LimitTime > today  后减前-->
			<if test="today !=null and today !=''">
				and DATEDIFF(day,LimitTime, #{today}) <![CDATA[ < ]]> 0
			</if>						
		</where>
	</select>
	
	<select id="getTotalNumber" parameterType="Map" resultType="Long">
		select count(*) from DrugRepoStore
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
