	<select id="findDrugRoomInput" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRoomInput
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<!-- 根据入库操作员查询入库记录 -->
			<if test="executeempid !=null and executeempid !=''">
				and ExecuteEmpID = #{executeempid}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>
			<if test="inputstate !=null ">
				and InputState = #{inputstate}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by InputID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

