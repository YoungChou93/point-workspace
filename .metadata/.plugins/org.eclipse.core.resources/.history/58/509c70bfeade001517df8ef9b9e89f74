	<select id="findHosMedicineAdvice" parameterType="Map"
		resultMap="BaseResultMap">
		select * from HosMedicineAdvice
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID = #{serialid}
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="advicetype !=null ">
				and AdviceType = #{advicetype}
			</if>
			<!-- 判断病人是否满足出科条件 -->
			<if test="checkout !=null">
				and (AdviceState=0 or AdviceState=1 or
				AdviceState=2 or (AdviceState=4 and AdviceType=0) or
				AdviceState=3 or AdviceState=5 or AdviceState=7)
			</if>
			<if test="transfuse !=null">
				and ExecuteMethodID in (select MethodID from
				ExecuteMethod where IsNeedExecute='true')
				and AdviceState=4
			</if>

		</where>
		<if test="start!=null and size!=null">
			order by AdviceType, AdviceID , SerialID
			offset ${start}
			rows fetch next ${size} rows only
		</if>
	</select>

	<select id="getTotalHosMedicineAdvice" parameterType="Map"
		resultType="Long">
		select count(*) from HosMedicineAdvice
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID = #{serialid}
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="advicetype !=null ">
				and AdviceType = #{advicetype}
			</if>
			<!-- 判断病人是否满足出科条件 -->
			<if test="checkout !=null">
				and (AdviceState=0 or AdviceState=1 or
				AdviceState=2 or (AdviceState=4 and AdviceType=0) or
				AdviceState=3 or AdviceState=5 or AdviceState=7)
			</if>
			<if test="transfuse !=null">
				and ExecuteMethodID in (select MethodID from
				ExecuteMethod where IsNeedExecute='true')
				and AdviceState=4
			</if>
		</where>
	</select>

	<select id="getMaxID" parameterType="Map" resultType="String">
		select MAX(SerialID) from HosMedicineAdvice
		<where>
			<if test="prefix !=null and prefix !=''">
				and SerialID like #{prefix}
			</if>
		</where>
	</select>

	<select id="getMaxAdviceID" parameterType="Map" resultType="String">
		select MAX(AdviceID) from HosMedicineAdvice
		<where>
			<if test="prefix !=null and prefix !=''">
				and AdviceID like #{prefix}
			</if>
		</where>
	</select>

	<select id="findHosMedicineAdvice4Execute" parameterType="Map"
		resultMap="BaseResultMap">
		select * from HosMedicineAdvice
		<where>
			<!-- 过滤得到护士分管的当前科室的病人 -->
			<if
				test="deptid !=null and deptid !='' and nurseid !=null and nurseid !=''">
				and HosCode in (
				select HosCode from HosDeptRegister where DeptID = #{deptid} and
				AdmissionsNurseID = #{nurseid}
				)
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="advicetype !=null ">
				and AdviceType = #{advicetype}
			</if>
			<if test="datenum !=null and datenum !=''">
				and ExecuteTimeNm = #{datenum}
			</if>
			<!-- 长期医嘱 传入islong 加载处于传送状态或执行状态的医嘱 -->
			<if test="islong !=null ">
				and (AdviceState = 2 or AdviceState = 4 )
			</if>
			<!-- 临时医嘱医嘱 传入istemp 只加载处于传送状态的临时医嘱 -->
			<if test="istemp !=null ">
				and AdviceState = 2
			</if>
		</where>
	</select>

	<select id="getHosCodeByAdvice" parameterType="java.lang.String" resultMap="BaseResultMap">
		select top 1 * from HosMedicineAdvice where AdviceID = #{adviceid,jdbcType=CHAR}
		order by SerialID
	</select>

	<select id="findHosMedicineAdviceByAdviceID" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		select * from HosMedicineAdvice where AdviceID =
		#{adviceid,jdbcType=CHAR}
	</select>

	<select id="findHosMedicineAdvice4Nurse" parameterType="Map"
		resultMap="BaseResultMap">
		select * from HosMedicineAdvice
		<where>
			<!-- 过滤得到护士分管的当前科室的病人 -->
			<if
				test="deptid !=null and deptid !='' and nurseid !=null and nurseid !=''">
				and HosCode in (
				select HosCode from HosDeptRegister where DeptID = #{deptid} and
				AdmissionsNurseID = #{nurseid}
				)
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<if test="advicetype !=null ">
				and AdviceType = #{advicetype}
			</if>
			<!-- 转抄 -->
			<if test="ensure !=null ">
				and AdviceState = 7
			</if>
			<!-- 撤销 -->
			<if test="cancel !=null ">
				and AdviceState = 3
			</if>
			<!-- 停止 -->
			<if test="stop !=null ">
				and AdviceState = 5
			</if>
		</where>
	</select>
</mapper>