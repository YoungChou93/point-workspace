	<select id="findHospitalizationInvoice" parameterType="Map"
		resultMap="BaseResultMap">
		select * from HospitalizationInvoice
		<where>
			<!-- 按住院发票分类名称查询 -->
			<if test="hospitalizationname!=null and hospitalizationname!=''">
				and HospitalizationName like #{hospitalizationname}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by HospitalizationID offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalHospitalizationInvoice" parameterType="Map"
