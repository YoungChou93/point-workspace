				and OutputState = ${outputstate}
			</if>
			<if test="executeempid !=null and executeempid !=''">
				and ExecuteEmpID = #{executeempid}
			</if>
		</where>
	</select>

	<select id="getMaxID" parameterType="Map" resultType="String">
		select MAX(OutputID) from DrugRepoOutput
		<where>
			<if test="prefix !=null and prefix !=''">
				and OutputID like #{prefix}
			</if>
		</where>
	</select>

	<select id="getBillCode" parameterType="Map" resultType="String">
		select MAX(BillCode) from DrugRepoOutput
		<where>
			<if test="prefix !=null and prefix !=''">
				and BillCode like #{prefix}
			</if>
		</where>
	</select>
	
	<select id="getOutputStateByBillCode" parameterType="java.lang.String" resultType="Integer">
		select top 1 OutputState from DrugRepoOutput where BillCode = #{billcode,jdbcType=CHAR}
	</select>
	
	<select id="getCntByPlanID" parameterType="java.lang.String" resultType="Integer">
		select count(*) from DrugRepoOutput where OutputType = 0 and PlanID = #{planid,jdbcType=CHAR}
	</select>
