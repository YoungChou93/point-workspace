	<select id="findDrugRepoOutput" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRepoOutput
		<where>
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<if test="destdept !=null and destdept !=''">
				and DestDeptID = #{destdept}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>
			<if test="outputstate !=null ">
				and OutputState = ${outputstate}
			</if>
			<if test="executeempid !=null and executeempid !=''">
				and ExecuteEmpID = #{executeempid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by OutputID offset ${start} rows fetch next ${size}
			rows only
		</if>
	</select>

	<select id="getTotalDrugRepoOutput" parameterType="Map"
		resultType="Long">
		select count(*) from DrugRepoOutput
		<where>
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<if test="destdept !=null and destdept !=''">
