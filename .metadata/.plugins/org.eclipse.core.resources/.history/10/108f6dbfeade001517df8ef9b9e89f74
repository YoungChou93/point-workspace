	<select id="findHosArchiveOtherCost" parameterType="Map"
		resultMap="BaseResultMap">
		select * from HosArchiveOtherCost
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID=#{serialid}
			</if>
			<if test="hoscode !=null and hoscode !=''">
				and HosCode = #{hoscode}
			</if>
			<!-- 用于检索需要打印发票的记录 -->
			<if test="invoicestate != null">
				and InvoiceState = ${invoicestate}
			</if>
			<if test="coststate != null">
				and CostState = ${coststate}
			</if>
			<!-- 用于检索正常收费的项目 -->
			<if test="costmode != null">
				and CostState >=0
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by SerialID DESC offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

	<select id="getTotalHosArchiveOtherCost" parameterType="Map"
		resultType="Long">
		select count(*) from HosArchiveOtherCost
		<where>
			<if test="serialid !=null and serialid !=''">
				and SerialID=#{serialid}
			</if>
		</where>
	</select>

	<select id="getSerialID" parameterType="Map" resultType="String">
		select MAX(SerialID) from HosArchiveOtherCost
		<where>
			<if test="prefix !=null and prefix !=''">
				and SerialID like #{prefix}
			</if>
		</where>
	</select>
