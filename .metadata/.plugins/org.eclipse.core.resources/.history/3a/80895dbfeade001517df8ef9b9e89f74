	<select id="findDrugRepoChangePrice" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRepoChangePrice
		<where>
			<if test="storeid !=null and storeid !=''">
				and StoreID = #{storeid}
			</if>
			<if test="changepriceid !=null and changepriceid !=''">
				and ChangePriceID = #{changepriceid}
			</if>
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>
			<if test="medicinepycode !=null and medicinepycode !=''">
				and MedicineID in ( 
				select MedicineID from MedicineBaseInfo 
					where ( PyCode like #{medicinepycode} or WbCode like #{medicinepycode} or 
							SelfDefCode like #{medicinepycode} or MedicineName like #{medicinepycode})
				)
			</if>
			<if test="changepriceemp !=null and changepriceemp !=''">
				and ChangePriceEmpID = #{changepriceemp}
			</if>
			<if test="changepricestate !=null ">
				and ChangePriceState = ${changepricestate}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by ChangePriceID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

	<select id="getTotalDrugRepoChangePrice" parameterType="Map"
		resultType="Long">
		select count(*) from DrugRepoChangePrice
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="changepriceid !=null and changepriceid !=''">
				and ChangePriceID = #{changepriceid}
			</if>
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>
			<if test="medicinepycode !=null and medicinepycode !=''">
				and MedicineID in ( 
				select MedicineID from MedicineBaseInfo 
					where ( PyCode like #{medicinepycode} or WbCode like #{medicinepycode} or 
							SelfDefCode like #{medicinepycode} or MedicineName like #{medicinepycode})
				)
			</if>
			<if test="changepriceemp !=null and changepriceemp !=''">
				and ChangePriceEmpID = #{changepriceemp}
			</if>
			<if test="changepricestate !=null ">
				and ChangePriceState = ${changepricestate}
			</if>			
		</where>
	</select>

	<select id="getMaxID" parameterType="Map" resultType="String">
		select MAX(ChangePriceID) from DrugRepoChangePrice
		<where>
			<if test="prefix !=null and prefix !=''">
				and ChangePriceID like #{prefix}
			</if>
		</where>
	</select>
	
	<select id="getBillCode" parameterType="Map" resultType="String">
