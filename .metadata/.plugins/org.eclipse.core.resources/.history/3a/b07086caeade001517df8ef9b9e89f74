	<select id="findBedCost" parameterType="Map" resultMap="BaseResultMap">
		select * from BedCost
		<where>
			<if test="bedid !=null and bedid !=''">
				and BedID = #{bedid}
			</if>
			<if test="costid !=null and costid !=''">
				and OtherItemCostID = #{costid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by ID offset ${start} rows fetch next ${size}
			rows
			only
		</if>
