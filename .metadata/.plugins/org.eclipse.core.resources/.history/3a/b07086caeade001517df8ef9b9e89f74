		<include refid="Base_Column_List" />
		from BedCost
		where ID = #{id,jdbcType=CHAR}
	</select>

	<select id="selectByMap" parameterType="Map" resultMap="BaseResultMap">
		select * from BedCost
		<where>
			<if test="bedid !=null and bedid !=''">
				and BedID = #{bedid}
			</if>
		</where>
	</select>

	<delete id="deleteByPrimaryKey" parameterType="java.lang.String">
		delete from
		BedCost
		where ID = #{id,jdbcType=CHAR}
	</delete>

	<delete id="deleteByMap" parameterType="Map">
		delete from BedCost
		<where>
			<if test="bedid !=null and bedid !=''">
				and BedID = #{bedid}
			</if>
		</where>
	</delete>

	<insert id="insertSelective" parameterType="com.his.entity.baseinfo.BedCost">
		insert into BedCost
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null">
				ID,
			</if>
			<if test="bed != null">
				BedID,
			</if>
			<if test="cost != null">
				OtherItemCostID,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
