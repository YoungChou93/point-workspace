	<select id="findServiceItemReturnApply" parameterType="Map"
		resultMap="BaseResultMap">
		select * from ServiceItemReturnApply
		<where>
			<if test="applyid !=null and applyid !=''">
				and ApplyID=#{applyid}
			</if>
			<if test="iccardid !=null and iccardid !=''">
				and ICCardID=#{iccardid}
			</if>
			<if test="patientid !=null and patientid !=''">
				and PatientID=#{patientid}
			</if>
			<if test="markid !=null and markid !=''">
				and MarkID=#{markid}
			</if>
			<if test="deptid !=null and deptid !=''">
				and ReceiveDeptID=#{deptid}
			</if>
			<if test="applystate !=null">
				and ApplyState= ${applystate}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by MarkID DESC offset ${start} rows fetch next
			${size} rows only
		</if>
	</select>

