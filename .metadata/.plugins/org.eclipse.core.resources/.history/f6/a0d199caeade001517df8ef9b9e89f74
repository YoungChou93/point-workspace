	<select id="findTestInformation" parameterType="Map" resultMap="BaseResultMap">
		select * from TestInformation
		<where>
			<!-- 按检查分类名称查询 -->
			<if test="testinfname!=null and testinfname!=''">
				and TestInfName like #{testinfname}
			</if>
			<!-- 按检查分类ID查询 -->
			<if test="testinfid!=null and testinfid!=''">
				and TestInfID like #{testinfid}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by TestInfID offset ${start} rows fetch next ${size}
			rows only
		</if>
	</select>

	<select id="getTotalTestInformation" parameterType="Map"
		resultType="Long">
		select count(*) from TestInformation
		<where>
			<if test="testinfid!=null and testinfid!='' ">
				and TestInfID like #{testinfid}
			</if>
		</where>
	</select>

	<select id="selectMaxtestinfid" resultType="String">
		select MAX(TestInfID)
		from TestInformation
	</select>
