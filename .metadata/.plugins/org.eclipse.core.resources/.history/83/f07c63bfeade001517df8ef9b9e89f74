	<select id="findDrugRoomCheck" parameterType="Map" resultMap="BaseResultMap">
		select * from DrugRoomCheck
		<where>
			<if test="medicineid !=null and medicineid !=''">
				and MedicineID = #{medicineid}
			</if>
			<if test="billcode !=null and billcode !=''">
				and BillCode = #{billcode}
			</if>		
			<if test="deptid !=null and deptid !=''">
				and DeptID = #{deptid}
			</if>
			<if test="checkemp !=null and checkemp !=''">
				and CheckEmpID = #{checkemp}
			</if>	
			<if test="checkstate !=null ">
				and CheckState = ${checkstate}
			</if>
		</where>
		<if test="start!=null and size!=null">
			order by CheckID offset ${start} rows fetch next ${size} rows only
		</if>
	</select>

